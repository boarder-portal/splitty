{
  "name": "splitty",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev-start": "npm-run-all --parallel server:watch client:watch",
    "prod-start": "npm-run-all --sequential client:build server:start",
    "client:build": "webpack",
    "client:watch": "webpack --watch",
    "server:start": "ts-node -r tsconfig-paths/register ./app/server/server.ts",
    "server:watch": "ts-node-dev -r tsconfig-paths/register ./app/server/server.ts",
    "analize-bundle": "ANALYZE_BUNDLE=1 webpack",
    "generateDB": "ts-node -r tsconfig-paths/register app/server/db/generateDB.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "alias": {
    "server": "./app/server",
    "client": "./app/client",
    "common": "./app/common"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boarder-portal/splitty.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boarder-portal/splitty/issues"
  },
  "homepage": "https://github.com/boarder-portal/splitty#readme",
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@types/connect-redis": "0.0.14",
    "@types/redis": "^2.8.25",
    "bem-cn": "^3.0.1",
    "connect-redis": "^5.0.0",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fs-extra": "^9.0.0",
    "pug": "^2.0.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "redis": "^3.0.2",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "styled-reset": "^4.1.4",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^9.0.0",
    "@types/morgan": "^1.7.37",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.3",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import-helpers": "1.0.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.2.2",
    "tsconfig-paths": "^3.9.0",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0"
  }
