  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch-server watch-client",
    "watch-client": "parcel watch app/client/client.tsx --out-dir ./build",
    "watch-server": "ts-node-dev -r tsconfig-paths/register ./app/server/server.ts",
    "typecheck": "tsc",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "alias": {
    "server": "./app/server",
    "client": "./app/client",
    "common": "./app/common"
  },
