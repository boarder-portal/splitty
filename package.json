  },
  "homepage": "https://github.com/boarder-portal/splitty#readme",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "apollo-server": "^2.10.1",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "parcel-bundler": "^1.12.4",
    "pug": "^2.0.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
